{
  "name": "fluid.js",
  "version": "0.1.0",
  "description": "A lightweight, zero-dependency fluid simulation for web backgrounds that reacts to device motion.",
  "type": "module",
  "main": "./dist/fluid.js.umd.js",
  "module": "./dist/fluid.js.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/fluid.js.es.js",
      "require": "./dist/fluid.js.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && cp src/index.d.ts dist/index.d.ts",
    "build:demo": "vite build -c vite.demo.config.js",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fluid",
    "simulation",
    "canvas",
    "background",
    "gyroscope",
    "physics",
    "liquid",
    "ui"
  ],
  "devDependencies": {
    "dat.gui": "^0.7.9",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": {
    "name": "mgks",
    "url": "https://mgks.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgks/fluid.js.git"
  },
  "bugs": {
    "url": "https://github.com/mgks/fluid.js/issues"
  },
  "homepage": "https://github.com/mgks/fluid.js#readme",
  "funding": "https://github.com/sponsors/mgks",
  "license": "MIT"
}